	<update id="updateXlbBalance" parameterClass="xlbBalance">
		UPDATE XLB_BALANCE@b2c_link
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="memberId">
              MEMBER_ID=#memberId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="amount">
              AMOUNT=#amount#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="inviteNo">
              INVITE_NO=#inviteNo#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="status">
              STATUS=#status#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateTime">
              UPDATE_TIME=#updateTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateUser">
              UPDATE_USER=#updateUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="filed1">
              FILED1=#filed1#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="filed2">
              FILED2=#filed2#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="filed3">
              FILED3=#filed3#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="filed4">
              FILED4=#filed4#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="filed5">
