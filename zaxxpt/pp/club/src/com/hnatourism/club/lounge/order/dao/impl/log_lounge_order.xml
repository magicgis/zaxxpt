		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	<sql id="selectLogLoungeOrderSql">
		<![CDATA[
			SELECT T.ID,T.ORDER_ID,T.CONTENT,T.TYPE,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME 
			FROM LOG_LOUNGE_ORDER T
		]]>
	</sql>
	<select id="findLogLoungeOrderByWhere" parameterClass="logLoungeOrder" resultMap="logLoungeOrderResultMap">
		<include refid="selectLogLoungeOrderSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="orderId">
				T.ORDER_ID=#orderId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="content">
				T.CONTENT=#content#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="type">
				T.TYPE=#type#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findLogLoungeOrderById" parameterClass="java.lang.String" resultMap="logLoungeOrderResultMap">
		<include refid="selectLogLoungeOrderSql"/>	
		<![CDATA[
			WHERE T.ID=#id# 
		]]>
	</select>
	
	<select id="findLogByOrderId" parameterClass="java.lang.String" resultMap="logLoungeOrderResultMap">
		<include refid="selectLogLoungeOrderSql"/>	
		<![CDATA[
			WHERE T.ORDER_ID=#orderId# ORDER BY CREATE_TIME DESC
		]]>
	</select>	
	
