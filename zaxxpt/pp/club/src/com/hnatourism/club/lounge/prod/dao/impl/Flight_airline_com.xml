<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="flightAirlineCom">
	<typeAlias alias="flightAirlineCom" type="com.hnatourism.club.lounge.prod.domain.FilghtAirlineCom" />
	<resultMap id="flightAirlineComResultMap" class="flightAirlineCom">
		<result column="ID" property="id" />
		<result column="CODE" property="code" />
		<result column="NAME" property="name" />
	</resultMap>
	<sql id="selectFlightAirlineComSql">
		<![CDATA[
			SELECT T.ID,T.CODE,T.NAME 
			FROM FLIGHT_AIRLINE_COMPANY@B2C_LINK T
		]]>
	</sql>
	<select id="findFlightAirlineComByCode" parameterClass="java.lang.String" resultMap="flightAirlineComResultMap">
		<include refid="selectFlightAirlineComSql"/>	
		<![CDATA[
			WHERE T.CODE=#code# AND T.STS=1
		]]>
	</select>
</sqlMap>