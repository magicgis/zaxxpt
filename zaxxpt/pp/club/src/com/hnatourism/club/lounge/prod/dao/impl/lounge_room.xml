			</isNotEmpty>


			<isNotEmpty prepend="," property="bookDate">
				#bookDate#
			</isNotEmpty>


			<isNotEmpty prepend="," property="startTime">
				#startTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="endTime">
				#endTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="startDate">
				#startDate#
			</isNotEmpty>


			<isNotEmpty prepend="," property="endDate">
				#endDate#
			</isNotEmpty>


