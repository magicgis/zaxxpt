	<resultMap id="loungeRoomResultMapHome" class="loungeRoom">
		<result column="ID" property="id" />
		<result column="LOUNGE_ID" property="loungeId" />
		<result column="TYPE" property="type" />
	</resultMap>
	<sql id="selectLoungeRoomSql">
		<![CDATA[
			SELECT T.ID,T.LOUNGE_ID,T.TYPE,T.ROOM_TYPE,T.STS,T.QUANTITY,T.BOOK_DATE,T.START_TIME,T.END_TIME,T.START_DATE,T.END_DATE,T.RMK,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME,T.ADVANCE_HOURS,T.NAME 
			FROM LOUNGE_ROOM T
		]]>
	</sql>
	<select id="findLoungeRoomByWhere" parameterClass="loungeRoom" resultMap="loungeRoomResultMap">
		<include refid="selectLoungeRoomSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="loungeId">
				T.LOUNGE_ID=#loungeId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="type">
				T.TYPE=#type#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="roomType">
				T.ROOM_TYPE=#roomType#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="sts">
				T.STS=#sts#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="quantity">
				T.QUANTITY=#quantity#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="bookDate">
				T.BOOK_DATE=#bookDate#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="startTime">
				T.START_TIME=#startTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="endTime">
				T.END_TIME=#endTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="startDate">
				T.START_DATE=#startDate#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="endDate">
				T.END_DATE=#endDate#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="rmk">
				T.RMK=#rmk#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="advanceHours">
				T.ADVANCE_HOURS=#advanceHours#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				T.NAME=#name#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findLoungeRoomById" parameterClass="java.lang.String" resultMap="loungeRoomResultMap">
		<include refid="selectLoungeRoomSql"/>	
		<![CDATA[
			WHERE T.ID=#id# AND T.STS=1
		]]>
	</select>
	<select id="findLoungeRoomByComm" parameterClass="java.lang.String" resultMap="loungeRoomResultMapHome">
		<include refid="selectLoungeRoomSql"/>	
		<![CDATA[
			WHERE T.ID IN (select PROD_CODE from RECOMMEND_PROD rp where rp.PROD_TYPE='L' and rp.STS=1) AND T.STS=1
		]]>
	</select>
	<select id="findLoungeRoomBySearch" parameterClass="loungeRoom" resultMap="loungeRoomResultMap">
		<include refid="selectLoungeRoomSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="loungeId">
				 T.LOUNGE_ID=#loungeId# AND T.STS=1
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="type">
				 T.TYPE=#type# AND T.STS=1
			</isNotEmpty>
		</dynamic>
		<![CDATA[
			ORDER BY T.TYPE,T.ROOM_TYPE
		]]>
	</select>
	<select id="findLoungeRoomByLoungeHome" parameterClass="loungeRoom" resultMap="loungeRoomResultMapHome">
		<include refid="selectLoungeRoomSql"/>	
		<![CDATA[
			WHERE T.LOUNGE_ID=#loungeId# AND T.STS=1
		]]>
	</select>
	<insert id="insertLoungeRoom" parameterClass="loungeRoom">
