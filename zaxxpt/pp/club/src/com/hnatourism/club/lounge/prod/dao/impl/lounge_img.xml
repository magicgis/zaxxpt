<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="loungeImg">
	<typeAlias alias="loungeImg" type="com.hnatourism.club.lounge.prod.domain.LoungeImg" />
	<resultMap id="loungeImgResultMap" class="loungeImg">
		<result column="ID" property="id" />
		<result column="LOUNGE_ID" property="loungeId" />
		<result column="TYPE" property="type" />
		<result column="NAME" property="name" />
		<result column="PATH" property="path" />
		<result column="STS" property="sts" />
		<result column="RMK" property="rmk" />
		<result column="SEQ" property="seq" />
		<result column="CREATE_USER" property="createUser" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="UPDATE_USER" property="updateUser" />
		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	<sql id="selectLoungeImgSql">
		<![CDATA[
			SELECT T.ID,T.LOUNGE_ID,T.TYPE,T.NAME,T.PATH,T.STS,T.RMK,T.SEQ,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME 
			FROM LOUNGE_IMG T
		]]>
	</sql>
	<select id="findLoungeImgByWhere" parameterClass="loungeImg" resultMap="loungeImgResultMap">
		<include refid="selectLoungeImgSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="loungeId">
				T.LOUNGE_ID=#loungeId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="type">
				T.TYPE=#type#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				T.NAME=#name#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="path">
				T.PATH=#path#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="sts">
				T.STS=#sts#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="rmk">
				T.RMK=#rmk#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="seq">
				T.SEQ=#seq#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findLoungeImgById" parameterClass="java.lang.String" resultMap="loungeImgResultMap">
		<include refid="selectLoungeImgSql"/>	
		<![CDATA[
			WHERE T.ID=#id# AND T.STS=1
		]]>
	</select>
	<select id="findLoungeImgBySearch" parameterClass="java.lang.String" resultMap="loungeImgResultMap">
		<include refid="selectLoungeImgSql"/>	
		<![CDATA[
			WHERE T.LOUNGE_ID=#loungeId# AND T.STS=1 AND T.TYPE='0'
		]]>
	</select>
	<insert id="insertLoungeImg" parameterClass="loungeImg">
		INSERT INTO LOUNGE_IMG
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="loungeId">
				LOUNGE_ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="type">
				TYPE
			</isNotEmpty>

			<isNotEmpty prepend="," property="name">
				NAME
			</isNotEmpty>

			<isNotEmpty prepend="," property="path">
				PATH
			</isNotEmpty>

			<isNotEmpty prepend="," property="sts">
				STS
			</isNotEmpty>

			<isNotEmpty prepend="," property="rmk">
				RMK
			</isNotEmpty>

			<isNotEmpty prepend="," property="seq">
				SEQ
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateUser">
				UPDATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateTime">
				UPDATE_TIME
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>


			<isNotEmpty prepend="," property="loungeId">
				#loungeId#
			</isNotEmpty>


			<isNotEmpty prepend="," property="type">
				#type#
			</isNotEmpty>


			<isNotEmpty prepend="," property="name">
				#name#
			</isNotEmpty>


			<isNotEmpty prepend="," property="path">
				#path#
			</isNotEmpty>


			<isNotEmpty prepend="," property="sts">
				#sts#
			</isNotEmpty>


			<isNotEmpty prepend="," property="rmk">
				#rmk#
			</isNotEmpty>


			<isNotEmpty prepend="," property="seq">
				#seq#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateUser">
				#updateUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateTime">
				#updateTime#
			</isNotEmpty>

		 </dynamic>	)
	</insert>
	
	<update id="updateLoungeImg" parameterClass="loungeImg">
		UPDATE LOUNGE_IMG
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="loungeId">
              LOUNGE_ID=#loungeId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="type">
              TYPE=#type#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="name">
              NAME=#name#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="path">
              PATH=#path#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="sts">
              STS=#sts#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="rmk">
              RMK=#rmk#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="seq">
              SEQ=#seq#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateUser">
              UPDATE_USER=#updateUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateTime">
              UPDATE_TIME=#updateTime#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	<delete id="deleteLoungeImg" parameterClass="java.lang.String">
		DELETE FROM LOUNGE_IMG
		WHERE 
               ID=#id#
	</delete>	
</sqlMap>