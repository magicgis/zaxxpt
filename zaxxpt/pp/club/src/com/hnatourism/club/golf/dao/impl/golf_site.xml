		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	<sql id="selectGolfSiteSql">
		<![CDATA[
			SELECT T.ID,T.GOLF_ID,T.PATTERN,T.TYPE,T.NUM,T.NAME,T.START_TIME,T.END_TIME,T.RMK,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME 
			FROM GOLF_SITE T
		]]>
	</sql>
	<select id="findGolfSiteByWhere" parameterClass="golfSite" resultMap="golfSiteResultMap">
		<include refid="selectGolfSiteSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="golfId">
				T.GOLF_ID=#golfId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="pattern">
				T.PATTERN=#pattern#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="type">
				T.TYPE=#type#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="num">
				T.NUM=#num#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				T.NAME=#name#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="startTime">
				T.START_TIME=#startTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="endTime">
				T.END_TIME=#endTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="rmk">
				T.RMK=#rmk#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findGolfSiteById" parameterClass="java.lang.String" resultMap="golfSiteResultMap">
		<include refid="selectGolfSiteSql"/>	
		<![CDATA[
			WHERE T.ID=#id#
		]]>
	</select>
	<insert id="insertGolfSite" parameterClass="golfSite">
