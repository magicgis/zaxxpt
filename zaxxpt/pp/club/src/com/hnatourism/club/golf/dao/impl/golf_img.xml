	<update id="updateGolfImg" parameterClass="golfImg">
		UPDATE GOLF_IMG
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="rmk">
              RMK=#rmk#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="golfId">
              GOLF_ID=#golfId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="path">
