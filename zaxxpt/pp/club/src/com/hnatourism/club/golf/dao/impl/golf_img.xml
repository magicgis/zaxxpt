	<sql id="selectGolfImgSql">
		<![CDATA[
			SELECT T.RMK,T.ID,T.GOLF_ID,T.PATH,T.TYPE,T.STS,T.SEQ,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME 
			FROM GOLF_IMG T
		]]>
	</sql>
	<select id="findGolfImgByWhere" parameterClass="golfImg" resultMap="golfImgResultMap">
		<include refid="selectGolfImgSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="rmk">
				T.RMK=#rmk#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="golfId">
				T.GOLF_ID=#golfId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="path">
				T.PATH=#path#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="type">
				T.TYPE=#type#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="sts">
				T.STS=#sts#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="seq">
				T.SEQ=#seq#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findGolfImgById" parameterClass="java.lang.String" resultMap="golfImgResultMap">
		<include refid="selectGolfImgSql"/>	
		<![CDATA[
			$data.getById()
		]]>
	</select>
	<insert id="insertGolfImg" parameterClass="golfImg">
