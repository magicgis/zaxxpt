<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="golfTimeMainTainApi">
	<typeAlias alias="golfTimeMaintainVo" type="com.hnatourism.club.golf.prod.vo.GolfTimeMaintainVo" />
	<resultMap id="api_golftimeMaintainResultMap" class="golfTimeMaintainVo">
		<result column="ID" property="id" />
		<result column="GOLF_ID" property="golfId" />
		<result column="END_TIME" property="endTime" />
		<result column="START_TIME" property="startTime" />
		<result column="CREATE_USER" property="createUser" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="UPDATE_USER" property="updateUser" />
		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	
	<sql id="api_selectGolftimeMaintainSql">
		<![CDATA[
			SELECT T.ID,T.GOLF_ID,T.END_TIME,T.START_TIME,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME 
			FROM GOLF_TIME_MAINTAIN T
		]]>
	</sql>
	
	<select id="api_findGolftimeMaintainBySite" parameterClass="java.lang.String" resultMap="api_golftimeMaintainResultMap">
		<include refid="api_selectGolftimeMaintainSql"/>	
		<![CDATA[
			WHERE T.GOLF_ID =#id#
		]]>
	</select>
</sqlMap>