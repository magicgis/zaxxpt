<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="sysContact">
	<typeAlias alias="sysContact" type="com.hnatourism.club.common.domain.SysContact" />
	<resultMap id="sysContactResultMap" class="sysContact">
		<result column="ID" property="id" />
		<result column="SCOPE" property="scope" />
		<result column="NAME" property="name" />
		<result column="RMK" property="rmk" />
		<result column="MOBILE" property="mobile" />
		<result column="EMAIL" property="email" />
		<result column="PHONE" property="phone" />
		<result column="SEX" property="sex" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="CREATE_USER" property="createUser" />
		<result column="UPDATE_TIME" property="updateTime" />
		<result column="UPDATE_USER" property="updateUser" />
		<result column="ROLE" property="role" />
		<result column="SUB_ROLE" property="subRole" />
		<result column="FAX" property="fax" />
	</resultMap>
	<sql id="selectSysContactSql">
		<![CDATA[
			SELECT T.ID,T.SCOPE,T.NAME,T.RMK,T.MOBILE,T.EMAIL,T.PHONE,T.SEX,T.CREATE_TIME,T.CREATE_USER,T.UPDATE_TIME,T.UPDATE_USER,T.ROLE,T.SUB_ROLE,T.FAX 
			FROM SYS_CONTACT T
		]]>
	</sql>
	<select id="findSysContactByWhere" parameterClass="sysContact" resultMap="sysContactResultMap">
		<include refid="selectSysContactSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="scope">
				T.SCOPE=#scope#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				T.NAME=#name#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="rmk">
				T.RMK=#rmk#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="mobile">
				T.MOBILE=#mobile#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="email">
				T.EMAIL=#email#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="phone">
				T.PHONE=#phone#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="sex">
				T.SEX=#sex#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="role">
				T.ROLE=#role#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="subRole">
				T.SUB_ROLE=#subRole#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="fax">
				T.FAX=#fax#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findSysContactById" parameterClass="java.lang.String" resultMap="sysContactResultMap">
		<include refid="selectSysContactSql"/>	
		<![CDATA[
			WHERE T.ID=#id#
		]]>
	</select>
	<!-- 根据 gaolf产品id查询 联系人 -->
	<select id="findSysContactByGolf" parameterClass="java.lang.String" resultMap="sysContactResultMap">
		<include refid="selectSysContactSql"/>	
		<![CDATA[
			WHERE T.SCOPE=#golfid# AND T.SUB_ROLE='1'
		]]>
	</select>
	<insert id="insertSysContact" parameterClass="sysContact">
		INSERT INTO SYS_CONTACT
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="scope">
				SCOPE
			</isNotEmpty>

			<isNotEmpty prepend="," property="name">
				NAME
			</isNotEmpty>

			<isNotEmpty prepend="," property="rmk">
				RMK
			</isNotEmpty>

			<isNotEmpty prepend="," property="mobile">
				MOBILE
			</isNotEmpty>

			<isNotEmpty prepend="," property="email">
				EMAIL
			</isNotEmpty>

			<isNotEmpty prepend="," property="phone">
				PHONE
			</isNotEmpty>

			<isNotEmpty prepend="," property="sex">
				SEX
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateTime">
				UPDATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateUser">
				UPDATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="role">
				ROLE
			</isNotEmpty>

			<isNotEmpty prepend="," property="subRole">
				SUB_ROLE
			</isNotEmpty>

			<isNotEmpty prepend="," property="fax">
				FAX
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>


			<isNotEmpty prepend="," property="scope">
				#scope#
			</isNotEmpty>


			<isNotEmpty prepend="," property="name">
				#name#
			</isNotEmpty>


			<isNotEmpty prepend="," property="rmk">
				#rmk#
			</isNotEmpty>


			<isNotEmpty prepend="," property="mobile">
				#mobile#
			</isNotEmpty>


			<isNotEmpty prepend="," property="email">
				#email#
			</isNotEmpty>


			<isNotEmpty prepend="," property="phone">
				#phone#
			</isNotEmpty>


			<isNotEmpty prepend="," property="sex">
				#sex#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateTime">
				#updateTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateUser">
				#updateUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="role">
				#role#
			</isNotEmpty>


			<isNotEmpty prepend="," property="subRole">
				#subRole#
			</isNotEmpty>


			<isNotEmpty prepend="," property="fax">
				#fax#
			</isNotEmpty>

		 </dynamic>	)
	</insert>
	
	<update id="updateSysContact" parameterClass="sysContact">
		UPDATE SYS_CONTACT
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="scope">
              SCOPE=#scope#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="name">
              NAME=#name#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="rmk">
              RMK=#rmk#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="mobile">
              MOBILE=#mobile#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="email">
              EMAIL=#email#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="phone">
              PHONE=#phone#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="sex">
              SEX=#sex#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateTime">
              UPDATE_TIME=#updateTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateUser">
              UPDATE_USER=#updateUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="role">
              ROLE=#role#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="subRole">
              SUB_ROLE=#subRole#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="fax">
              FAX=#fax#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	<delete id="deleteSysContact" parameterClass="java.lang.String">
		DELETE FROM SYS_CONTACT
		WHERE 
               ID=#id#
	</delete>	
</sqlMap>