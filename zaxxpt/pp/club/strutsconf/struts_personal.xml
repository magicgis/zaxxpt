			<result name="successPage">/WEB-INF/pages/personal.member/success.jsp</result>
			<result name="failPage">/WEB-INF/pages/personal.member/fail.jsp</result>
		</action>
		<action name="memberRoleAction" class="com.hnatourism.club.personal.member.web.MemberRoleAction" method="search">
			<result name="addPage">/WEB-INF/pages/personal.member/addMemberRole.jsp</result>
			<result name="modifyPage">/WEB-INF/pages/personal.member/modifyMemberRole.jsp</result>
			<result name="searchPage">/WEB-INF/pages/personal.member/searchMemberRole.jsp</result>
			<result name="viewPage">/WEB-INF/pages/personal.member/viewMemberRole.jsp</result>
