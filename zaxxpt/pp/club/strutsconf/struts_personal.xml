			<result name="viewPage">/WEB-INF/pages/personal.member/viewMemberInfo.jsp</result>
			<result name="successPage">/WEB-INF/pages/personal.member/success.jsp</result>
			<result name="failPage">/WEB-INF/pages/personal.member/fail.jsp</result>
			<result name="success" type="redirect">/member/index.jsp</result>
			<result name="mistake">/WEB-INF/pages/member/login.jsp</result>
			<result name="error">/common/error.jsp</result>
		</action>
		<action name="memberInvoiceAction" class="com.hnatourism.club.personal.member.web.MemberInvoiceAction" method="search">
			<result name="addPage">/WEB-INF/pages/personal.member/addMemberInvoice.jsp</result>
