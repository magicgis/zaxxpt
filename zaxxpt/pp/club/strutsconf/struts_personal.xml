<struts>
	<package name="personal" extends="default">
		<action name="memberAccountAction" class="com.hnatourism.club.personal.member.web.MemberAccountAction" method="search">
			<result name="addPage">/WEB-INF/pages/personal.member/addMemberAccount.jsp</result>
			<result name="modifyPage">/WEB-INF/pages/personal.member/modifyMemberAccount.jsp</result>
			<result name="searchPage">/WEB-INF/pages/personal.member/searchMemberAccount.jsp</result>
			<result name="viewPage">/WEB-INF/pages/personal.member/viewMemberAccount.jsp</result>
			<result name="successPage">/WEB-INF/pages/personal.member/success.jsp</result>
			<result name="failPage">/WEB-INF/pages/personal.member/fail.jsp</result>
		</action>
		<action name="memberInfoAction" class="com.hnatourism.club.personal.member.action.MemberInfoAction" method="search">
