			<result name="failPage">/WEB-INF/pages/personal.member/fail.jsp</result>
		</action>
		<action name="memberPassengerAction" class="com.hnatourism.club.personal.member.web.MemberPassengerAction" method="search">
			<result name="addPage">/WEB-INF/pages/personal.member/addMemberPassenger.jsp</result>
			<result name="modifyPage">/WEB-INF/pages/personal.member/modifyMemberPassenger.jsp</result>
