<struts>
	<package name="pay" namespace="/pay" extends="struts-default">
		<action name="payAction" class="payAction">
			<result name="depOnlineSave">/WEB-INF/pages/pay/api_rechargeBack.jsp</result>
			<result name="toPayPage">/WEB-INF/pages/member/deposit.jsp</result>
			<!-- 登录限制	lixun -->
			<result name="notlogin" type="redirect">
				/member/login.jsp
			</result>
		</action>
		<action name="payBackAction" class="payBackAction">
		</action>
	</package>
</struts>
