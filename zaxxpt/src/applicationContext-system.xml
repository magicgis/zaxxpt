<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="loginAction" class="com.platform.login.action.LoginAction" scope="prototype" parent="baseAction"/>
	
	<bean id="userAction" class="com.system.user.action.UserAction" scope="prototype" parent="baseAction"/>

	<bean id="deptAction" class="com.system.dept.action.DeptAction" scope="prototype" parent="baseAction"/>

	<bean id="dictAction" class="com.system.dict.action.DictAction" scope="prototype" parent="baseAction"/>

	<bean id="dutyAction" class="com.system.duty.action.DutyAction" scope="prototype" parent="baseAction"/> 

	<bean id="roleAction" class="com.system.role.action.RoleAction" scope="prototype" parent="baseAction"/>

	<bean id="logAction" class="com.system.log.action.LogAction" scope="prototype" parent="baseAction"/>
</beans>