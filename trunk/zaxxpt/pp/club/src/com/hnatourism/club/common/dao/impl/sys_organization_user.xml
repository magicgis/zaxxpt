<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="sysOrganizationUser">
	<typeAlias alias="sysOrganizationUser" type="com.hnatourism.club.common.domain.SysOrganizationUser" />
	<resultMap id="sysOrganizationUserResultMap" class="sysOrganizationUser">
		<result column="ID" property="id" />
		<result column="ORGANIZATION_ID" property="organizationId" />
		<result column="USER_ID" property="userId" />
		<result column="CREATE_USER" property="createUser" />
		<result column="CREATE_TIME" property="createTime" />
	</resultMap>
	<sql id="selectSysOrganizationUserSql">
		<![CDATA[
			SELECT T.ID,T.ORGANIZATION_ID,T.USER_ID,T.CREATE_USER,T.CREATE_TIME 
			FROM SYS_ORGANIZATION_USER T
		]]>
	</sql>
	<select id="findSysOrganizationUserByWhere" parameterClass="sysOrganizationUser" resultMap="sysOrganizationUserResultMap">
		<include refid="selectSysOrganizationUserSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="organizationId">
				T.ORGANIZATION_ID=#organizationId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="userId">
				T.USER_ID=#userId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findSysOrganizationUserById" parameterClass="java.lang.String" resultMap="sysOrganizationUserResultMap">
		<include refid="selectSysOrganizationUserSql"/>	
		<![CDATA[
			WHERE T.ID=#id#
		]]>
	</select>
	<insert id="insertSysOrganizationUser" parameterClass="sysOrganizationUser">
		INSERT INTO SYS_ORGANIZATION_USER
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="organizationId">
				ORGANIZATION_ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="userId">
				USER_ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>


			<isNotEmpty prepend="," property="organizationId">
				#organizationId#
			</isNotEmpty>


			<isNotEmpty prepend="," property="userId">
				#userId#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>

		 </dynamic>	)
	</insert>
	
	<update id="updateSysOrganizationUser" parameterClass="sysOrganizationUser">
		UPDATE SYS_ORGANIZATION_USER
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="organizationId">
              ORGANIZATION_ID=#organizationId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="userId">
              USER_ID=#userId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	<delete id="deleteSysOrganizationUser" parameterClass="java.lang.String">
		DELETE FROM SYS_ORGANIZATION_USER
		WHERE 
               ID=#id#
	</delete>	
</sqlMap>