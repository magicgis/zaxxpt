	</insert>
	
	<update id="updateGolfInfo" parameterClass="golfInfo">
		UPDATE GOLF_INFO
        <dynamic prepend="set">
        	<isNotEmpty prepend="," property="pattern">
              PATTERN=#pattern#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="name">
