<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="golfInfo">
	<typeAlias alias="golfInfo" type="com.hnatourism.club.golf.domain.GolfInfo" />
	<resultMap id="golfInfoResultMap" class="golfInfo">
