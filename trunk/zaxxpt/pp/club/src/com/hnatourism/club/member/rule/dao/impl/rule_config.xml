<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="ruleConfig">
	<typeAlias alias="ruleConfig" type="com.hnatourism.club.member.rule.domain.RuleConfig" />
	<resultMap id="ruleConfigResultMap" class="ruleConfig">
		<result column="STS" property="sts" />
		<result column="END_TIME" property="endTime" />
		<result column="START_TIME" property="startTime" />
		<result column="ID" property="id" />
		<result column="TYPE" property="type" />
		<result column="NAME" property="name" />
		<result column="CODE" property="code" />
		<result column="VALUE" property="value" />
		<result column="RMK" property="rmk" />
		<result column="CREATE_USER" property="createUser" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="UPDATE_USER" property="updateUser" />
		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	<sql id="selectRuleConfigSql">
		<![CDATA[
			SELECT T.STS,T.END_TIME,T.START_TIME,T.ID,T.TYPE,T.NAME,T.CODE,T.VALUE,T.RMK,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME 
			FROM RULE_CONFIG T
		]]>
	</sql>
	<select id="findRuleConfigByWhere" parameterClass="ruleConfig" resultMap="ruleConfigResultMap">
		<include refid="selectRuleConfigSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="sts">
				T.STS=#sts#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="endTime">
				T.END_TIME=#endTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="startTime">
				T.START_TIME=#startTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="type">
				T.TYPE=#type#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				T.NAME=#name#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="code">
				T.CODE=#code#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="value">
				T.VALUE=#value#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="rmk">
				T.RMK=#rmk#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findRuleConfigById" parameterClass="java.lang.String" resultMap="ruleConfigResultMap">
		<include refid="selectRuleConfigSql"/>	
		<![CDATA[
			$data.getById()
		]]>
	</select>
	<select id="findRuleConfigByCode" parameterClass="java.lang.String" resultMap="ruleConfigResultMap">
		<include refid="selectRuleConfigSql"/>	
		<![CDATA[
			T.CODE=#code# AND T.STS=1
		]]>
	</select>
	<select id="findRuleConfigByCrop" parameterClass="java.lang.String" resultMap="ruleConfigResultMap">
		<include refid="selectRuleConfigSql"/>	
		<![CDATA[
			T.CROP=#crop# AND T.STS=1
		]]>
	</select>
	<insert id="insertRuleConfig" parameterClass="ruleConfig">
		INSERT INTO RULE_CONFIG
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="sts">
				STS
			</isNotEmpty>

			<isNotEmpty prepend="," property="endTime">
				END_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="startTime">
				START_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="type">
				TYPE
			</isNotEmpty>

			<isNotEmpty prepend="," property="name">
				NAME
			</isNotEmpty>

			<isNotEmpty prepend="," property="code">
				CODE
			</isNotEmpty>

			<isNotEmpty prepend="," property="value">
				VALUE
			</isNotEmpty>

			<isNotEmpty prepend="," property="rmk">
				RMK
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateUser">
				UPDATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateTime">
				UPDATE_TIME
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="sts">
				#sts#
			</isNotEmpty>


			<isNotEmpty prepend="," property="endTime">
				#endTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="startTime">
				#startTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>


			<isNotEmpty prepend="," property="type">
				#type#
			</isNotEmpty>


			<isNotEmpty prepend="," property="name">
				#name#
			</isNotEmpty>


			<isNotEmpty prepend="," property="code">
				#code#
			</isNotEmpty>


			<isNotEmpty prepend="," property="value">
				#value#
			</isNotEmpty>


			<isNotEmpty prepend="," property="rmk">
				#rmk#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateUser">
				#updateUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateTime">
				#updateTime#
			</isNotEmpty>

		 </dynamic>	)
	</insert>
	
	<update id="updateRuleConfig" parameterClass="ruleConfig">
		UPDATE RULE_CONFIG
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="sts">
              STS=#sts#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="endTime">
              END_TIME=#endTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="startTime">
              START_TIME=#startTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="type">
              TYPE=#type#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="name">
              NAME=#name#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="code">
              CODE=#code#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="value">
              VALUE=#value#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="rmk">
              RMK=#rmk#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateUser">
              UPDATE_USER=#updateUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateTime">
              UPDATE_TIME=#updateTime#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	<delete id="deleteRuleConfig" parameterClass="java.lang.String">
		DELETE FROM RULE_CONFIG
		WHERE 
               ID=#id#
	</delete>	
</sqlMap>