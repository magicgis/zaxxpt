<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="golfTimeMaintain">
	<typeAlias alias="golfTimeMaintain" type="com.hnatourism.club.golf.domain.GolfTimeMaintain" />
	<resultMap id="golfTimeMaintainResultMap" class="golfTimeMaintain">
		<result column="ID" property="id" />
		<result column="GOLF_ID" property="golfId" />
		<result column="END_TIME" property="endTime" />
		<result column="START_TIME" property="startTime" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="CREATE_USER" property="createUser" />
		<result column="UPDATE_USER" property="updateUser" />
		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	<sql id="selectGolfTimeMaintainSql">
		<![CDATA[
			SELECT T.ID,T.GOLF_ID,T.END_TIME,T.START_TIME,T.CREATE_TIME,T.CREATE_USER,T.UPDATE_USER,T.UPDATE_TIME 
			FROM GOLF_TIME_MAINTAIN T
		]]>
	</sql>
	<select id="findGolfTimeMaintainByWhere" parameterClass="golfTimeMaintain" resultMap="golfTimeMaintainResultMap">
		<include refid="selectGolfTimeMaintainSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="golfId">
				T.GOLF_ID=#golfId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="endTime">
				T.END_TIME=#endTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="startTime">
				T.START_TIME=#startTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findGolfTimeMaintainById" parameterClass="java.lang.String" resultMap="golfTimeMaintainResultMap">
		<include refid="selectGolfTimeMaintainSql"/>	
		<![CDATA[
			WHERE T.ID=#id#
		]]>
	</select>
	<insert id="insertGolfTimeMaintain" parameterClass="golfTimeMaintain">
		INSERT INTO GOLF_TIME_MAINTAIN
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="golfId">
				GOLF_ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="endTime">
				END_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="startTime">
				START_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateUser">
				UPDATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateTime">
				UPDATE_TIME
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>


			<isNotEmpty prepend="," property="golfId">
				#golfId#
			</isNotEmpty>


			<isNotEmpty prepend="," property="endTime">
				#endTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="startTime">
				#startTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateUser">
				#updateUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateTime">
				#updateTime#
			</isNotEmpty>

		 </dynamic>	)
	</insert>
	
	<update id="updateGolfTimeMaintain" parameterClass="golfTimeMaintain">
		UPDATE GOLF_TIME_MAINTAIN
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="golfId">
              GOLF_ID=#golfId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="endTime">
              END_TIME=#endTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="startTime">
              START_TIME=#startTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateUser">
              UPDATE_USER=#updateUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateTime">
              UPDATE_TIME=#updateTime#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	<delete id="deleteGolfTimeMaintain" parameterClass="java.lang.String">
		DELETE FROM GOLF_TIME_MAINTAIN
		WHERE 
               ID=#id#
	</delete>	
</sqlMap>