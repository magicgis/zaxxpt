<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="api_orderGlofPlayer">
	<typeAlias alias="golfOrderPlayVo" type="com.hnatourism.club.golf.order.vo.GolfOrderPlayVo" />
	<resultMap id="api_orderGlofPlayerResultMap" class="golfOrderPlayVo">
		<result column="ID" property="id" />
		<result column="ORDER_ID" property="orderId" />
		<result column="NAME" property="name" />
		<result column="STS" property="sts" />
		<result column="RMK" property="rmk" />
		<result column="ORDER_TYPE" property="orderType" />
		<result column="CREATE_USER" property="createUser" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="UPDATE_USER" property="updateUser" />
		<result column="UPDATE_TIME" property="updateTime" />
	</resultMap>
	<sql id="api_selectOrderGlofPlayerSql">
		<![CDATA[
			SELECT T.ID,T.ORDER_ID,T.NAME,T.STS,T.RMK,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME,T.ORDER_TYPE
			FROM ORDER_GLOF_PLAYER T
		]]>
	</sql>
	<select id="api_findOrderGlofPlayerByWhere" parameterClass="golfOrderPlayVo" resultMap="api_orderGlofPlayerResultMap">
		<include refid="api_selectOrderGlofPlayerSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="orderId">
				T.ORDER_ID=#orderId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				T.NAME=#name#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="sts">
				T.STS=#sts#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="rmk">
				T.RMK=#rmk#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="api_findOrderGlofPlayerById" parameterClass="java.lang.String" resultMap="api_orderGlofPlayerResultMap">
		<include refid="api_selectOrderGlofPlayerSql"/>	
		<![CDATA[
			WHERE T.ORDER_ID=#id#
		]]>
	</select>
	
	<select id="api_findGlofPlayerById" parameterClass="java.lang.String" resultMap="api_orderGlofPlayerResultMap">
		<include refid="api_selectOrderGlofPlayerSql"/>	
		<![CDATA[
			WHERE T.ID=#id#
		]]>
	</select>
	
	<parameterMap class="java.util.HashMap" id="insertParameterMap">   
      <parameter   property="id" />
		<parameter   property="orderId" />
		<parameter   property="name" />
		<parameter  property="sts" />
		<parameter   property="rmk" />
		<parameter   property="createUser" />
		<parameter   property="createTime" />
		<parameter   property="updateUser" />
		<parameter   property="updateTime" />
    </parameterMap>
	
	<insert id="api_saveOrderGlofPlayer" parameterMap="insertParameterMap">
		INSERT INTO ORDER_GLOF_PLAYER
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="orderId">
				ORDER_ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="name">
				NAME
			</isNotEmpty>

			<isNotEmpty prepend="," property="sts">
				STS
			</isNotEmpty>

			<isNotEmpty prepend="," property="rmk">
				RMK
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateUser">
				UPDATE_USER
			</isNotEmpty>
			
			<isNotEmpty prepend="," property="updateTime">
				UPDATE_TIME
			</isNotEmpty>
			
			<isNotEmpty prepend="," property="orderType">
				ORDER_TYPE
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>

			<isNotEmpty prepend="," property="orderId">
				#orderId#
			</isNotEmpty>

			<isNotEmpty prepend="," property="name">
				#name#
			</isNotEmpty>

			<isNotEmpty prepend="," property="sts">
				#sts#
			</isNotEmpty>

			<isNotEmpty prepend="," property="rmk">
				#rmk#
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateUser">
				#updateUser#
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateTime">
				#updateTime#
			</isNotEmpty>
			
			<isNotEmpty prepend="," property="orderType">
				#orderType#
			</isNotEmpty>
		
		 </dynamic>	)
	</insert>
	
	<update id="modifyOrderGlofPlayer" parameterClass="golfOrderPlayVo">
		UPDATE ORDER_GLOF_PLAYER
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="orderId">
              ORDER_ID=#orderId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="name">
              NAME=#name#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="sts">
              STS=#sts#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="rmk">
              RMK=#rmk#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateUser">
              UPDATE_USER=#updateUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateTime">
              UPDATE_TIME=#updateTime#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	
</sqlMap>