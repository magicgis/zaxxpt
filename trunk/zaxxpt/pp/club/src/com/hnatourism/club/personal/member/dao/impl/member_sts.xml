<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="memberSts">
	<typeAlias alias="memberSts" type="com.hnatourism.club.personal.member.domain.MemberSts" />
	<resultMap id="memberStsResultMap" class="memberSts">
		<result column="ID" property="id" />
		<result column="MEMBER_ID" property="memberId" />
		<result column="STS" property="sts" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="CREATE_USER" property="createUser" />
		<result column="UPDATE_TIME" property="updateTime" />
		<result column="UPDATE_USER" property="updateUser" />
	</resultMap>
	<sql id="selectMemberStsSql">
		<![CDATA[
			SELECT T.ID,T.MEMBER_ID,T.STS,T.CREATE_TIME,T.CREATE_USER,T.UPDATE_TIME,T.UPDATE_USER 
			FROM MEMBER_STS T
		]]>
	</sql>
	<select id="findMemberStsByWhere" parameterClass="memberSts" resultMap="memberStsResultMap">
		<include refid="selectMemberStsSql"/>	
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="AND" property="memberId">
				T.MEMBER_ID=#memberId#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="sts">
				T.STS=#sts#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateUser">
				T.UPDATE_USER=#updateUser#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findMemberStsById" parameterClass="java.lang.String" resultMap="memberStsResultMap">
		<include refid="selectMemberStsSql"/>	
		<![CDATA[
			WHERE T.ID=#id#
		]]>
	</select>
	<select id="findMemberStsByMember" parameterClass="java.lang.String" resultMap="memberStsResultMap">
		<include refid="selectMemberStsSql"/>	
		<![CDATA[
			WHERE T.MEMBER_ID=#memberId#
		]]>
	</select>
	<insert id="insertMemberSts" parameterClass="memberSts">
		INSERT INTO MEMBER_STS
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="memberId">
				MEMBER_ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="sts">
				STS
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateTime">
				UPDATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="updateUser">
				UPDATE_USER
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>


			<isNotEmpty prepend="," property="memberId">
				#memberId#
			</isNotEmpty>


			<isNotEmpty prepend="," property="sts">
				#sts#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateTime">
				#updateTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="updateUser">
				#updateUser#
			</isNotEmpty>

		 </dynamic>	)
	</insert>
	
	<update id="updateMemberSts" parameterClass="memberSts">
		UPDATE MEMBER_STS
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="memberId">
              MEMBER_ID=#memberId#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="sts">
              STS=#sts#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateTime">
              UPDATE_TIME=#updateTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="updateUser">
              UPDATE_USER=#updateUser#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	<delete id="deleteMemberSts" parameterClass="java.lang.String">
		DELETE FROM MEMBER_STS
		WHERE 
               ID=#id#
	</delete>	
</sqlMap>