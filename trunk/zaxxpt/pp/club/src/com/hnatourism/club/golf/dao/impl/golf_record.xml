			</isNotEmpty>
			<isNotEmpty prepend="AND" property="updateTime">
				T.UPDATE_TIME=#updateTime#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findGolfRecordById" parameterClass="java.lang.String" resultMap="golfRecordResultMap">
