	<sql id="selectGolfRecordSql">
		<![CDATA[
			SELECT T.ID,T.SITE_ID,T.CONTRACT_ID,T.ADD_NUM,T.START_TIME,T.END_TIME,T.CONTRACT_PRICE,T.PRICE,T.AMOUNT,T.STS,T.CREATE_USER,T.CREATE_TIME,T.UPDATE_USER,T.UPDATE_TIME 
			FROM GOLF_RECORD T
		]]>
	</sql>
	<select id="findGolfRecordByWhere" parameterClass="golfRecord" resultMap="golfRecordResultMap">
		<include refid="selectGolfRecordSql"/>	
		<dynamic prepend="WHERE">
