<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">

<sqlMap namespace="memberRole">
	<typeAlias alias="memberRole" type="com.hnatourism.club.personal.member.domain.MemberRole" />
	<resultMap id="memberRoleResultMap" class="memberRole">
		<result column="ID" property="id" />
		<result column="CODE" property="code" />
		<result column="NAME" property="name" />
		<result column="CREATE_TIME" property="createTime" />
		<result column="CREATE_USER" property="createUser" />
		<result column="STS" property="sts" />
	</resultMap>
	<sql id="selectMemberRoleSql">
		<![CDATA[
			SELECT T.ID,T.CODE,T.NAME,T.CREATE_TIME,T.CREATE_USER,T.STS 
			FROM MEMBER_ROLE T
		]]>
	</sql>
	<select id="findMemberRoleByWhere" parameterClass="memberRole" resultMap="memberRoleResultMap">
		<include refid="selectMemberRoleSql"/>	
		<dynamic prepend="WHERE">
		<isNotEmpty prepend="AND" property="id">
				T.ID=#id#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="code">
				T.CODE=#code#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="name">
				T.NAME=#name#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createTime">
				T.CREATE_TIME=#createTime#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="createUser">
				T.CREATE_USER=#createUser#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="sts">
				T.STS=#sts#
			</isNotEmpty>
		</dynamic>
	</select>
	<select id="findMemberRoleById" parameterClass="java.lang.String" resultMap="memberRoleResultMap">
		<include refid="selectMemberRoleSql"/>	
		<![CDATA[
			WHERE T.ID=#id#
		]]>
	</select>
	<select id="findMemberRoleByMember" parameterClass="java.lang.String" resultMap="memberRoleResultMap">
		<include refid="selectMemberRoleSql"/>	
		<![CDATA[
			WHERE T.ID=#memberCode#
		]]>
	</select>
	<insert id="insertMemberRole" parameterClass="memberRole">
		INSERT INTO MEMBER_ROLE
		<dynamic prepend="(">
			<isNotEmpty prepend="," property="id">
				ID
			</isNotEmpty>

			<isNotEmpty prepend="," property="code">
				CODE
			</isNotEmpty>

			<isNotEmpty prepend="," property="name">
				NAME
			</isNotEmpty>

			<isNotEmpty prepend="," property="createTime">
				CREATE_TIME
			</isNotEmpty>

			<isNotEmpty prepend="," property="createUser">
				CREATE_USER
			</isNotEmpty>

			<isNotEmpty prepend="," property="sts">
				STS
			</isNotEmpty>

		</dynamic>		
		<dynamic prepend=") VALUES (">

			<isNotEmpty prepend="," property="id">
				#id#
			</isNotEmpty>


			<isNotEmpty prepend="," property="code">
				#code#
			</isNotEmpty>


			<isNotEmpty prepend="," property="name">
				#name#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createTime">
				#createTime#
			</isNotEmpty>


			<isNotEmpty prepend="," property="createUser">
				#createUser#
			</isNotEmpty>


			<isNotEmpty prepend="," property="sts">
				#sts#
			</isNotEmpty>

		 </dynamic>	)
	</insert>
	
	<update id="updateMemberRole" parameterClass="memberRole">
		UPDATE MEMBER_ROLE
        <dynamic prepend="set">
		   <isNotEmpty prepend="," property="code">
              CODE=#code#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="name">
              NAME=#name#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createTime">
              CREATE_TIME=#createTime#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="createUser">
              CREATE_USER=#createUser#
           </isNotEmpty>
		   <isNotEmpty prepend="," property="sts">
              STS=#sts#
           </isNotEmpty>
        </dynamic>
		WHERE 
               ID=#id#
	</update>	
	<delete id="deleteMemberRole" parameterClass="java.lang.String">
		DELETE FROM MEMBER_ROLE
		WHERE 
               ID=#id#
	</delete>	
</sqlMap>